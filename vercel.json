{
  "version": 2,
  "public": true,
  "name": "boite-a-ref",
  "rewrites": [
    {
      "source": "/assets/(.*)",
      "destination": "/assets/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ],
  "functions": {
    "api/index.js": {
      "includeFiles": "dist/boite-a-ref/**"
    }
  }
}
